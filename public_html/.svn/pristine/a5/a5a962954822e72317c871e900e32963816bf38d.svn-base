$(document).on('change','.uploadImage_1',function(){
	var id=  $(this).attr('data');
            
            $("#uploadPreview_"+id).show();
            $("#uploadPreview_"+id).attr('src',"http://localhost/btm/assets/img/circleloading.gif");
            
            var fd = new FormData();
            $.each($(this)[0].files, function (i, file) {
                fd.append('file-' + i, file);
            });
            fd.append('fname',"_picture"+id);

            $.ajax({
                url: 'http://localhost/btm/event/upload_resize_image',
                data: fd,
                cache: false,
                processData: false,
                contentType: false,
                context: this,
                type: 'POST',
                success: function (dat) {
                    dat = JSON.parse(dat);
                    
                    if (dat['status'] == 'success')
                    {
                        $("#uploadPreview_"+id).show();
                        $("#uploadPreview_"+id).attr('src', 'http://localhost/btm/assets/uploads/event_images/' + dat['file_name']);
                        $("#server_picture_image_"+id).val(dat['file_name']);
                        
                        $('#upimg_'+id).show();			
			            $('#upimg_'+id+' div .select-areas-background-area').css('background-image', '');
                        //alert(dat['file_name']);
                        var image   =   "http://localhost/btm/assets/uploads/event_images/"+dat['file_name'];
						$('#upimg_'+id+' div .select-areas-background-area').css('background-image', 'url('+image+')');
						
                        $("#cropimg_"+id).trigger('click');
                    } else if (dat['status'] == 'error') {
                        reset($(this));
                        var error = dat['error_msg'];
                    
                        $("#divid").html('<div class="alert alert-danger flash-row"><button class="close" data-dismiss="alert"><i class="ace-icon fa fa-times"></i></button><i class="ace-icon fa fa-check green"></i><strong>Error!</strong> '+error+' </div>');
	  	  
                         $("#uploadPreview_"+id).attr('src','');
                         $("#uploadPreview_"+id).hide();
                        $('html, body').animate({scrollTop: 0}, 'slow');
                    }

                }
            });
        });

