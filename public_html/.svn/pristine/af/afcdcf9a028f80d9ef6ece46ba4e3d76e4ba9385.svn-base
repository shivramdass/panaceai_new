#popup_title {
    font-size: 16px;
    color: #fff;
    line-height: 22px;
    background: #f8cb00;
    text-align: center;
    font-weight: bold;
    margin: 0px 0px;
    padding: 10px 10px 10px 10px;
    border-radius: 5px 5px 0px 0px;
    background-position:top center;
    background-repeat: no-repeat;
}

#popup_content {
	/*background: 16px 16px no-repeat url('../images/info.gif');*/
	padding: 30px 0px;
	margin: 0em;
}

