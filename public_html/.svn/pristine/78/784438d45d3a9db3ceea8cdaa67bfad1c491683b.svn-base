.big-text h1{ font-size:45px; margin-bottom:0; margin-top:5px; font-weight:bold; }
.big-text small{ font-size:18px; margin-bottom:10px; display:inline-block; color:#666;}

.my-mar .portlet{ margin-bottom:10px; padding:7px 10px; box-sizing:border-box}
.my-date-picker .datepicker-inline, .my-date-picker .datepicker table{ width:100% !important;}
.datepicker-inline, .table-condensed{ width:100% !important;}
.viewall{ margin:12px 0 0; text-transform:uppercase; font-size:18px; padding:5px 15px;}


.progress{ height:10px !important; }
.circle-rup{color:#fff;height:45px; width:45px; text-align:center; border-radius:2px; margin-top:20px; padding-top:18px;}
.circle-rup .fa{font-size:22px; margin-top:-15px; display:inline-block;}
.earning h1{ margin:0 0 5px 0; font-size:25px;}
.earning h1 .fa{ font-size:25px;}
.earning p{ font-size:20px; margin-bottom:5px; font-weight:400; color:#444;}
.earning .progress{ margin-bottom:5px;color:#fff}
.my-mar .bg-success{ background:#45B6AF; color:#fff}
.my-mar .bg-info{ background:#89C4F4;color:#fff}
.my-mar .bg-warning{ background:#DFBA49;color:#fff}
.my-mar .bg-danger{ background:#f36a5a;color:#fff}

.task-list ul{ list-style:none; margin:0; padding:0}
.task-list ul li{ border-bottom:1px dashed #f1f1f1; padding-bottom:5px; margin-bottom:5px;}

.task-list ul li .text{ width:75%; float:left;}
.task-list ul li .text p.name{ font-size:16px; font-weight:500; margin-bottom:3px; color:#666}
.task-list ul li .text p.location{ font-size:15px; color:#666;}
.circle-icon{height: 35px;
    width: 35px;
    border-radius: 100%;
    float: right; padding: 2px 0;
    text-align: center;
    font-size: 18px;
    margin-right: 10px;
    margin-top: 7px;
    color: #fff;
    line-height: normal;}
.date-time{ float:left; margin-top:12px;}
.date-time .time{ text-align:center; padding:3px 10px; border:1px solid #f1f1f1; margin-bottom:0;}
.date-time .days{    text-align: center;
    padding: 3px 10px;
    /* border: 1px solid #f1f1f1; */
    border-top: 0px;
    font-weight: bold;
    background: #6897D2;
    margin-top: 6px;
    width: 100%;
    display: inline-block;
    font-weight: 500;
    color: #FFF;}


.rupee-circle{ text-align:center;background:#ebeef0;  border-radius:100%; padding:25px;  border-radius:100%;}
.rupee-circle .fa{ font-size:40px; text-align:center; line-height:normal; color:#3598dc; margin-top:15px; display:inline-block;}

    .item-body {
    color: #96a5aa !important;
    width: 70%;
    float: right;
    margin-right: 10px;
    padding-top: 6px;
}

ul.top_tab li a.active .badge{    background-color: #fff;    color: #4E4A4A;
    font-weight: 600;}
	.btn-home {
    background: #ccc;
    padding: 5px;
    font-size: 11px;
    float: left;
    color: #333;
}